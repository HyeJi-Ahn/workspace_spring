package com.ezen.spring.polymorphism;

public class LgTV implements TV{
	public void on() {
		System.out.println("LgTV --- 전원 켜기");
	}
	
	public void off() {
		System.out.println("LgTV --- 전원 끄기");
	}
	
	public void up() {
		System.out.println("LgTV --- 소리 증가");
	}
	
	public void down() {
		System.out.println("LgTV --- 소리 감소");
	}
	
}
