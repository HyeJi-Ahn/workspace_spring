package com.ezen.spring.polymorphism;

public class TvUser {

	public static void main(String[] args) {
		TV tv;
        tv = new SamsungTV();
		
		SamsungTV sTv = new SamsungTV();
		tv.on();
		tv.up();
		tv.down();
		tv.off();
		
		LgTV lTv = new LgTV();
		lTv.on();
		lTv.up();
		lTv.down();
		lTv.off();
	}
}
