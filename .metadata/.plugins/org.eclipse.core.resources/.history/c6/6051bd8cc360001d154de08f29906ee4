package com.ezen.spring.polymorphism;

public class TvUser {

	public static void main(String[] args) {
		SamsungTV sTv = new SamsungTV();
		sTv.powerOn();
		sTv.volumeUp();
		sTv.volumeDown();
		sTv.powerOff();
		
		LgTV lTv = new LgTV();
		lTv.turnOn();
		lTv.soundUp();
		lTv.soundDown();
		lTv.turnOff();
		
		
	}

}
